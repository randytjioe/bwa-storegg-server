{
  "version": 2,
  "builds": [
    {
      "src": "./bin/www",
      "use": "@vercel/node"
    },
    
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/bin/www",
      "methods": ["GET", "PUT", "POST", "DELETE", "OPTIONS", "PATCH"]
    }
  ]
}
